{
  "name": "multi-gym-cjs",
  "version": "1.0.0",
  "description": "Sistema de Gesti√≥n para MultiGym CJS",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Fight Academy Scorpions",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.multigym.cjs",
    "productName": "MultiGym CJS",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist"
    ],
    "extraResources": [
      {
        "from": "resources/icono.ico",
        "to": "resources/icono.ico"
      }
    ],
    "win": {
      "target": "portable",
      "icon": "resources/icono.ico"
    }
  }
}
